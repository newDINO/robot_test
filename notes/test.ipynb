   "source": [
    "from sympy import *\n",
    "\n",
    "a, b, c, x = symbols('a b c x')\n",
    "eq = Eq(a * sin(x) + b * cos(x), c)\n",
    "solutions = solve(eq, x)\n",
    "\n",
    "x1 = solutions[0]\n",
    "x2 = solutions[1]\n",
    "display(x1)\n",
    "display(x2)\n",
    "print(x1)\n",
    "print(x2)"
   ]
